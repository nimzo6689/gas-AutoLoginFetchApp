{
  "name": "gas-auto-login-fetch-app",
  "version": "0.0.0",
  "description": "UrlFetchApp with auto-login functionality.",
  "main": "src/index.ts",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "npm run license && eslint --fix --no-error-on-unmatched-pattern src/ test/",
    "bundle": "rollup --no-treeshake -c rollup.config.mjs",
    "build": "npm run clean && npm run bundle && node ./scripts/split-bundle.mjs && ncp appsscript.json dist/appsscript.json",
    "license": "license-check-and-add add -f license-config.json",
    "test": "jest test/ --passWithNoTests --detectOpenHandles",
    "deploy": "npm run lint && npm run test && npm run build && ncp .clasp-dev.json .clasp.json && clasp push -f",
    "deploy:prod": "npm run lint && npm run test && npm run build && ncp .clasp-prod.json .clasp.json && clasp push"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/google-apps-script": "^1.0.77",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "license-check-and-add": "^4.0.5",
    "ncp": "^2.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
